<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.dowp.importer" ExtensionBundleVersion="1.1.5" ExtensionBundleName="DowP Importer">
    <ExtensionList>
        <Extension Id="com.dowp.importer" Version="1.1.5" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PPRO" Version="[14.0,99.9]" />
            <Host Name="AEFT" Version="[17.0,99.9]" />
        </HostList>
        <CSXS-RequiredProperties>
            <Property Name="PlayerDebugMode">1</Property>
            <Property Name="RequiredScriptAccess">full</Property>
            <Property Name="EnableNetworkAccess">true</Property>
        </CSXS-RequiredProperties>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <Runtime Name="CSXS" Version="7.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.dowp.importer">
            <DispatchInfo>
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <ScriptPath>./js/extendscript.js</ScriptPath>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>DowP Importer</Menu>
                    <Geometry>
                        <Size>
                            <Height>250</Height>
                            <Width>200</Width>
                        </Size>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icons/icon.png</Icon>
                        <Icon Type="DarkNormal">./icons/icon.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>